<%= form_with model: Contact, url: contact_path, local: true do |f| %>
  <div class="field">
    <%= f.hidden_field :user_id %><br>
  </div>
  <div class="field">
    <%= f.hidden_field :engineer_id %><br>
  </div>
  <div class="field">
    <%= f.hidden_field :language_id %><br>
  </div>
  <div class="form-group">
    <%= f.hidden_field :content %>
  </div><br>
  <div class="actions">
    <%= f.submit 'Back!', name: 'back' %>
  </div>
  <div class="actions">
    <%= f.submit 'create!' %>
  </div>
<% end %>